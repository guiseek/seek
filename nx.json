{
  "npmScope": "@seek-peer",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "peer-core": {
      "tags": []
    },
    "peer-signaling": {
      "tags": ["side:server", "type:feature", "scope:peer"]
    },
    "peer-client": {
      "tags": ["side:client", "type:feature", "scope:peer"]
    }
  }
}
